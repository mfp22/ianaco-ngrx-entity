{
  "name": "personal-website",
  "version": "0.0.0",
  "scripts": {
    "deploy": "git add --all && git commit -m 'auto-update' && git push -u origin main",
    "bundle:analyze": "webpack-bundle-analyzer docs/stats.json",
    "postbuild:move": "npm run deploy",
    "build:move": "move-cli ./temp/404.html ./docs/404.html && rmdir temp",
    "build:copy": "cpy ./docs/index.html ./temp --flat --rename=404.html && npm run build:move",
    "build:git": "ng build --output-path=docs --stats-json --configuration production && npm run build:copy",
    "ng": "ng",
    "start": "ng serve --host 192.168.1.56",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/cdk": "^15.0.3",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/material": "^15.0.3",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "@ngrx/entity": "^15.1.0",
    "@ngrx/router-store": "^15.1.0",
    "@ngrx/store": "^15.1.0",
    "@ngrx/store-devtools": "^15.1.0",
    "hamburgers": "^1.2.1",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "^15.0.4",
    "@angular/compiler-cli": "^15.0.4",
    "@types/d3": "^7.1.0",
    "@types/jasmine": "^4.3.1",
    "@types/node": "^18.11.18",
    "cpy-cli": "^4.1.0",
    "jasmine-core": "^4.5.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "move-cli": "^2.0.0",
    "typescript": "^4.9.4",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
